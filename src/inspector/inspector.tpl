<div class="inspector-pane is-hidden">
  <header>
    <nav>
        <!-- <i class="fa fa-cog"></i> -->
      </nav>
  </header>
  <section>  
    <article>
      <input id='details-pane' type="checkbox" />
      <label for='details-pane'>Details</label>
      <dl>
        <dt>name</dt>
        <dd class='name'>{name}</dd>
        <dt>id</dt>
        <dd class='id'>{id}</dd>
        <dt>status</dt>
        <dd class='status'>{status}</dd>
        <dt>version</dt>
        <dd class='version'>unknown</dd>
        <dt>parent</dt>
        <dd class='parent'>unknown</dd>
      </dl>
    </article>
    <article>
      <input  id='data-pane' type="checkbox" />
      <label for='data-pane'>Data</label>
      <pre class='data'></pre>
    </article>
    <article>
      <input  id='listeners-pane' type="checkbox" />
      <label for='listeners-pane'>Listeners</label>
      <div class='listeners'></div>
    </article>
    <article>
      <input  id='recipients-pane' type="checkbox" />
      <label for='recipients-pane'>Recipients</label>
      <div class='recipients'></div>
    </article>
    <article>
      <input  id='child-nodes-pane' type="checkbox" />
      <label for='child-nodes-pane'>Child Nodes</label>
      <div class='child-nodes'></div>
    </article>
    <article>
      <input  id='history-pane' type="checkbox" />
      <label for='history-pane'>History</label>
      <div class="history"></div>
    </article>
  </section>

</div>
